		"@uiw/codemirror-theme-vscode": "^4.23.8",
		"@uiw/react-codemirror": "^4.23.8",
		"canvas-confetti": "^1.9.3",
		"html2canvas": "^1.4.1",
		"jspdf": "^3.0.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "^7.1.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.19.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
