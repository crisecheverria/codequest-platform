		"eslint-plugin-react-refresh": "^0.4.18",
		"globals": "^15.14.0",
		"jsdom": "^24.0.0",
		"postcss": "^8.5.2",
		"tailwindcss": "^3.4.1",
		"typescript": "~5.7.2",
		"typescript-eslint": "^8.22.0",
